@model IEnumerable<QL_CTDT.Data.Models.ViewModels.Nganh_VM>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ten)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoTa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TenKhoa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ten)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MoTa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenKhoa)
            </td>
            <td>
                <a asp-action="Edit" asp-controller="Nganhs" asp-route-id="@item.MaNganh">Edit</a> |
                <a asp-action="Details" asp-controller="Nganhs"  asp-route-id="@item.MaNganh">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaNganh">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
